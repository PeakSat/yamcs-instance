<?xml version="1.0" encoding="UTF-8"?>
<SpaceSystem name="adcs-dt" xmlns:xtce="http://www.omg.org/spec/XTCE/20180204"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.omg.org/spec/XTCE/20180204 https://www.omg.org/spec/XTCE/20180204/SpaceSystem.xsd"
	shortDescription="This is a bogus satellite telemetry and telecommand database."
	operationalStatus="unittest">

    <!-- Contains ADCS's non primitive parameters and arguments -->

    <xtce:TelemetryMetaData>
        <ParameterTypeSet>

            <IntegerParameterType name="ADCSMagnetometerRaw_t" signed="false">
				<IntegerDataEncoding encoding="unsigned" sizeInBits="16" />
                <DefaultAlarm>
					<StaticAlarmRanges>
						<WarningRange minInclusive="-5000" maxInclusive="5000" />
					</StaticAlarmRanges>
				</DefaultAlarm>
			</IntegerParameterType>

            <FloatParameterType name="ADCSMagnetometerFrequency_t">
                <FloatDataEncoding sizeInBits="32"/>
                    <DefaultAlarm>
                        <StaticAlarmRanges>
                            <WarningRange minExclusive="0" maxInclusive="150"/>
                        </StaticAlarmRanges>
                    </DefaultAlarm>
            </FloatParameterType>

            <FloatParameterType name="ADCSMagnetometerSelfTest_t">
                <FloatDataEncoding sizeInBits="32"/>
                    <DefaultAlarm>
                        <StaticAlarmRanges>
                            <WarningRange minInclusive="15" maxInclusive="500"/>
                        </StaticAlarmRanges>
                    </DefaultAlarm>
            </FloatParameterType>

            <FloatParameterType name="ADCSGyroscopeAndBoardTemperature_t">
                <FloatDataEncoding sizeInBits="32"/>
                    <DefaultAlarm>
                        <StaticAlarmRanges>
                            <WarningRange minInclusive="-40" maxInclusive="125"/>
                        </StaticAlarmRanges>
                    </DefaultAlarm>
            </FloatParameterType>

            <FloatParameterType name="ADCSMCUTemperature_t">
                <FloatDataEncoding sizeInBits="32"/>
                    <DefaultAlarm>
                        <StaticAlarmRanges>
                            <WarningRange minInclusive="-55" maxInclusive="125"/>
                        </StaticAlarmRanges>
                    </DefaultAlarm>
            </FloatParameterType>

        </ParameterTypeSet>  
    </xtce:TelemetryMetaData>

    <xtce:CommandMetaData>
        <ArgumentTypeSet>




        </ArgumentTypeSet>
    </xtce:CommandMetaData>

</SpaceSystem>